{
    "name": "opencfp/opencfp",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=5.5",
        "aptoma/twig-markdown": "0.2.*",
        "cartalyst/sentry": "2.1.*",
        "doctrine/dbal": "~2.3",
        "ezyang/htmlpurifier": "dev-master",
        "igorw/config-service-provider": "~1.2",
        "illuminate/container": "4.0.*@dev",
        "illuminate/database": "4.0.*@dev",
        "illuminate/events": "4.0.*@dev",
        "illuminate/support": "4.0.*@dev",
        "intervention/image": "1.5.*",
        "ircmaxell/password-compat": "1.0.*@dev",
        "ircmaxell/random-lib": "^1.1",
        "kzykhys/ciconia": "~1.0",
        "league/oauth2-server": "^4.1",
        "monolog/monolog": "^1.17",
        "pagerfanta/pagerfanta": "1.0.*@dev",
        "pimple/pimple": "1.*",
        "robmorgan/phinx": "*",
        "silex/silex": "1.*",
        "swiftmailer/swiftmailer": "5.0",
        "symfony/browser-kit": "~2.3",
        "symfony/config": "~2.3",
        "symfony/console": "~2.3",
        "symfony/css-selector": "~2.3",
        "symfony/form": "~2.3",
        "symfony/locale": "~2.3",
        "symfony/security-csrf": "~2.3",
        "symfony/translation": "~2.3",
        "symfony/twig-bridge": "~2.3",
        "symfony/validator": "~2.3",
        "symfony/yaml": "~2.5",
        "twig/twig": "1.*",
        "vlucas/spot2": "dev-master"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "0.2.0",
        "friendsofphp/php-cs-fixer": "^2.0.0",
        "fzaninotto/faker": "^1.5.0",
        "johnkary/phpunit-speedtrap": "~1.0@dev",
        "mockery/mockery": "1.0.*@dev",
        "phpunit/dbunit": "1.3.2",
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "OpenCFP\\": "classes/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/DatabaseTestCase.php"
        ],
        "psr-4": {
            "OpenCFP\\Test\\": "tests/"
        }
    }
